
public class Application {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//multidimentional array can have different number of elements like 
		int[][] grid = {{1,2,3},{2,3},{3}};
		System.out.println("grid[0][0]" + grid[0][0]);
		System.out.println("grid[0][1]" + grid[0][0]);
		System.out.println("grid[0][2]" + grid[0][0]);
		System.out.println("grid[1][0]" + grid[0][0]);
		System.out.println("grid[1][1]" + grid[0][0]);
		System.out.println("grid[2][0]" + grid[0][0]);
	}

}
